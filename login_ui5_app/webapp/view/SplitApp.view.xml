<mvc:View
    controllerName="LOGINUI5APP.loginui5app.controller.SplitApp"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:html="http://www.w3.org/1999/xhtml"
>
		<App>
			<pages>
			<SplitApp>
				<masterPages>
					<mvc:XMLView id="idMaster" viewName="LOGINUI5APP.loginui5app.view.Master"></mvc:XMLView>
				</masterPages>
				<detailPages>
					<mvc:XMLView id="idPersonal" viewName="LOGINUI5APP.loginui5app.view.Personal"></mvc:XMLView>
					<mvc:XMLView id="idExperience" viewName="LOGINUI5APP.loginui5app.view.Experience"></mvc:XMLView>
					<mvc:XMLView id="idAddress" viewName="LOGINUI5APP.loginui5app.view.Address"></mvc:XMLView>
				</detailPages>
			</SplitApp>
		</pages>
		</App>
</mvc:View>
